<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapActivity">


    <TextView
        android:id="@+id/node1"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/node2"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/node1"
        app:layout_constraintTop_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/node3"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/node2"
        app:layout_constraintTop_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/node4"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/node3"
        app:layout_constraintTop_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/node5"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/node4"
        app:layout_constraintTop_toBottomOf="parent"
        />
    <TextView
        android:id="@+id/node6"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/node5"
        app:layout_constraintTop_toBottomOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>